{% style %}
  .social_icons path {
    fill: currentColor;
  }
{% endstyle %}
<ul class="social_icons list-none flex gap-4 p-0">
  {%- if settings.social_facebook_link != blank -%}
    <li class="">
      <a href="{{ settings.social_facebook_link }}" class="ent-social-icon-wrapper">
        {%- render 'icon-facebook' -%}
        <span class="sr-only">{{ 'general.social.links.facebook' | t }}</span>
      </a>
    </li>
  {%- endif -%}
